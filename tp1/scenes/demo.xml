<sxs>
    <scene root="demoRoot" axis_length="5"/>

    <views default="defaultCamera">
        <perspective id="defaultCamera" near="0.1" far="500" angle="45">
            <from x="30" y="15  " z="30" />
            <to x="0" y="-2" z="0" />
        </perspective>
    </views>

    <ambient>
        <ambient r="0.1" g="0.1" b="0.15" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </ambient>

    <lights>
        <omni id="demoLight" enabled="1">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>
    </lights>

    <textures>
        <texture id="demoTexture" file="scenes/images/unknown.jpg" />
        <texture id="wallTexture" file="scenes/images/wall.jpg" />
        <texture id="floorTexture" file="scenes/images/floor.jpg" />

        <texture id="cabinFront" file="scenes/images/cabinetfront.jpg" />
        <texture id="cabinTop" file="scenes/images/cabinettop.jpg" />
        <texture id="cabinDef" file="scenes/images/cabinet.jpg" />
        
        <texture id="wood" file="scenes/images/wood.jpg" />

        <texture id="ovenFront" file="scenes/images/ovenfront.jpg" />
        <texture id="ovenTop" file="scenes/images/oventop.jpg" />
        <texture id="oven" file="scenes/images/oven.jpg" />
    </textures>

    <materials>
        <material id="basicMaterial" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
            <specular r="0.0" g="0.0" b="0.0" a="1.0" />
        </material>
        <material id="redMaterial" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
            <ambient r="1.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="0.8" g="0.5" b="0.5" a="1.0" />
            <specular r="0.0" g="0.0" b="0.0" a="1.0" />
        </material>
        <material id="metallic" shininess="100">
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
            <ambient r="0.5" g="0.5" b="0.5" a="0.5" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
            <specular r="1.0" g="0.5" b="0.5" a="1.0" />
        </material>
    </materials>

    <transformations>
      <transformation id="y90rot">
        <rotate axis="y" angle="90" />
      </transformation>
      <transformation id="y180rot">
        <rotate axis="y" angle="180" />
      </transformation>
      <transformation id="x90rot">
        <rotate axis="x" angle="-90" />
      </transformation>
      <transformation id="x-90rot">
        <rotate axis="x" angle="90" />
      </transformation>
      <transformation id="x180rot">
        <rotate axis="x" angle="180" />
      </transformation>
      <transformation id="invert">
        <scale x="-1" y="-1" z="-1" />
      </transformation>
      <transformation id="halfScale">
        <scale x="0.5" y="0.5" z="0.5" />
      </transformation>
    </transformations>

    <primitives>
      <primitive id="wall">
        <rectangle x1="0" y1="0" x2="15" y2="10" />
      </primitive>
      <primitive id="floor">
        <rectangle x1="0" y1="0" x2="15" y2="15" />
      </primitive>
      <primitive id="cabinLong">
        <rectangle x1="0" y1="0" x2="9" y2="2" />
      </primitive>
      <primitive id="cabinShort">
        <rectangle x1="0" y1="0" x2="2" y2="2" />
      </primitive>

      <primitive id="trig">
        <triangle x1="-2" y1="1" z1="0" x2="2" y2="1" z2="0" x3="0" y3 ="1" z3="-3.464" />
      </primitive>
      <primitive id="trigSide">
        <rectangle x1="-2" y1="0" x2="2" y2="1" />
      </primitive>

      <primitive id="chairLeg">
        <cylinder base="0.25" top="0.25" height="3" slices="6" stacks="3" />
      </primitive>
      <primitive id="tableTop">
        <rectangle x1="0" y1="0" x2="6" y2="4" />
      </primitive>
      <primitive id="tableSide">
        <rectangle x1="-3" y1="0" x2="3" y2="0.3" />
      </primitive>
      <primitive id="tableLeg">
        <cylinder base="0.2" top="0.13" height="3" slices="6" stacks="3" />
      </primitive>
      <primitive id="ovenSide">
        <rectangle x1="0" y1="0" x2="3" y2="3" />
      </primitive>
      <primitive id="bowl">
        <cylinder base="0.001" top="1" height="0.5" slices="10" stacks="3" />
      </primitive>

      <primitive id="testesphere">
        <sphere radius="2" slices="20" stacks="20" />
      </primitive>
    </primitives>

    <components>
        <component id="sceneRoot">
          <transformation>
            <translate x="0" y="0" z="0"/>
          </transformation>
            <materials>
                <material id="basicMaterial" />
            </materials>
          <texture id="demoTexture" length_s="1" length_t="1" />
            <children>
              <componentref id="walls" />
              <componentref id="cabin" />
              <componentref id="oven" />
              <componentref id="chair" />
              <componentref id="table" />
              <componentref id="bowl" />
            </children>
        </component>

      <component id="bowl">
        <transformation>
          <translate x="7" y="3" z="8" />
          <transformationref id="x90rot" />
        </transformation>
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="none"/>
        <children>
          <primitiveref id="bowl" />
        </children>
      </component>
      
      <component id="walls">
        <transformation>
          <translate x="0" y="0" z="0"/>
        </transformation>
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="wallTexture" length_s="1" length_t="1"/>
        <children>
          <primitiveref id="wall" />
          <primitiveref id="testesphere" />
          <componentref id="sideWallTrans" />
          <componentref id="floorTrans" />
        </children>
      </component>

      <component id="cabin">
        <transformation>
          <translate x="1" y="0" z="0"/>
        </transformation>
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="cabinDef" length_s="2" length_t="2"/>
        <children>
          <componentref id="topCabinTrans" />
          <componentref id="frontCabinTrans" />
          <componentref id="leftCabinTrans" />
        </children>
      </component>

      <component id="oven">
        <transformation>
          <translate x="1" y="0" z="0"/>
        </transformation>
        <materials>
            <material id="metallic" />
        </materials>
        <texture id="oven" length_s="3" length_t="3" />
        <children>
          <componentref id="topOvenTrans" />
          <componentref id="leftOvenTrans" /> 
          <componentref id="rightOvenTrans" /> 
          <componentref id="frontOvenTrans" />
        </children>
      </component>

      <component id="chair">
        <transformation>
          <translate x="0" y="0" z="0"/>
        </transformation>
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="wood" length_s="1" length_t="1" />
        <children>
          <componentref id="chairTopTrans" />
          <componentref id="chairFirstLeg" />
          <componentref id="chairSecondLeg" />
          <componentref id="chairThirdLeg" />
        </children>
      </component>

      <component id="depthTriangle">
        <transformation>
          <translate x="0" y="0" z="0"/>
        </transformation>
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <children>
          <primitiveref id="trig"/>
          <primitiveref id="trigSide"/>
          <componentref id="otherTrig" />
          <componentref id="trigSide1" />
          <componentref id="trigSide2" />
        </children>
      </component>

      <component id="trigSide1">
        <transformation>
          <translate x="1" y="0" z="-1.7" />
          <rotate axis="y" angle="120" />
        </transformation>
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <children>
          <primitiveref id="trigSide"/>
        </children>
      </component>

      <component id="trigSide2">
        <transformation>
          <translate x="-1" y="0" z="-1.7" />
          <rotate axis="y" angle="-120" />
        </transformation>
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <children>
          <primitiveref id="trigSide"/>
        </children>
      </component>

      <component id="otherTrig">
        <transformation>
          <translate x="0" y="1" z="0" />
          <rotate axis="z" angle="180" />
        </transformation>
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <children>
          <primitiveref id="trig"/>
        </children>
      </component>

      <component id="table">
        <transformation>
          <translate x="0" y="0" z="0"/>
        </transformation>
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="wood" length_s="1" length_t="1" />
        <children>
          <componentref id="tableTopTrans" />
          <componentref id="tableUnder" />
          <componentref id="tableLegs" />
          <componentref id="tableSidesLong" />
          <componentref id="tableSidesShort" />
        </children>
      </component>

      <component id="tableLegs">
        <transformation>
          <translate x="0" y="0" z="0"/>
        </transformation>
        <materials>
            <material id="redMaterial" />
        </materials>
        <texture id="none" />
        <children>
          <componentref id="tableFirstLeg" />
          <componentref id="tableSecondLeg" />
          <componentref id="tableThirdLeg" />
          <componentref id="tableFourthLeg" />
        </children>
      </component>

      <component id="tableTopTrans">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <translate x="6" y="3" z="9" />
          <transformationref id="x90rot"/>
        </transformation>
        <children>
          <primitiveref id="tableTop" />
        </children>
      </component>

      <component id="tableUnder">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <translate x="6" y="2.7" z="5" />
          <transformationref id="x-90rot"/>
        </transformation>
        <children>
          <primitiveref id="tableTop" />
        </children>
      </component>

      <component id="tableSidesShort">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <scale x="1.5" y="1" z="0.66667" />
          <translate x="-1" y="0" z="19.5" />
          <transformationref id="y90rot"/>
        </transformation>
        <children>
          <componentref id="tableSidesLong" />
        </children>
      </component>

      <component id="tableSidesLong">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <translate x="9" y="2.7" z="9" />
        </transformation>
        <children>
          <primitiveref id="tableSide" />
          <componentref id="tSideLongRev" />
        </children>
      </component>

      <component id="tSideLongRev">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <translate x="0" y="0" z="-4" />
          <transformationref id="y180rot" />
        </transformation>
        <children>
          <primitiveref id="tableSide" />
        </children>
      </component>

      <component id="tableFirstLeg">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <translate x="6.5" y="0" z="8.5" />
          <rotate axis="x" angle="-90" />
        </transformation>
        <children>
          <primitiveref id="tableLeg" />
        </children>
      </component>

      <component id="tableSecondLeg">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <translate x="6.5" y="0" z="5.5" />
          <rotate axis="x" angle="-90" />
        </transformation>
        <children>
          <primitiveref id="tableLeg" />
        </children>
      </component>

      <component id="tableThirdLeg">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <translate x="11.5" y="0" z="5.5" />
          <rotate axis="x" angle="-90" />
        </transformation>
        <children>
          <primitiveref id="tableLeg" />
        </children>
      </component>

      <component id="tableFourthLeg">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <translate x="11.5" y="0" z="8.5" />
          <rotate axis="x" angle="-90" />
        </transformation>
        <children>
          <primitiveref id="tableLeg" />
        </children>
      </component>

      <component id="sideWallTrans">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit"/>
        <transformation>
          <translate x="0" y="0" z="15" />
          <transformationref id="y90rot" />
        </transformation>
        <children>
          <primitiveref id="wall" />
        </children>
      </component>

      <component id="floorTrans">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="floorTexture" length_s="1" length_t="1"/>
        <transformation>
          <translate x="0" y="0" z="15" />
          <transformationref id="x90rot"/>
        </transformation>
        <children>
          <primitiveref id="floor" />
        </children>
      </component>
      <component id="topCabinTrans">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="cabinTop" length_s="3" length_t="2"/>
        <transformation>
          <translate x="0" y="2" z="2" />
          <transformationref id="x90rot"/>
        </transformation>
        <children>
          <primitiveref id="cabinLong" />
        </children>
      </component>

      <component id="frontCabinTrans">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="cabinFront" length_s="2.25" length_t="2"/>
        <transformation>
          <translate x="0" y="0" z="2" />
        </transformation>
        <children>
          <primitiveref id="cabinLong" />
        </children>
      </component>

      <component id="leftCabinTrans">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <translate x="0" y="0" z="0" />
          <transformationref id="y180rot"/>
          <transformationref id="y90rot"/>
        </transformation>
        <children>
          <primitiveref id="cabinShort" />
        </children>
      </component>

      <component id="topOvenTrans">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="ovenTop" length_s="3" length_t="3" />
        <transformation>
          <translate x="9" y="3" z="3" />
          <transformationref id="x90rot"/>
        </transformation>
        <children>
          <primitiveref id="ovenSide" />
        </children>
      </component>

      <component id="frontOvenTrans">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="ovenFront" length_s="3" length_t="3"/>
        <transformation>
          <translate x="9" y="0" z="3" />
        </transformation>
        <children>
          <primitiveref id="ovenSide" />
        </children>
      </component>

      <component id="rightOvenTrans">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <translate x="12" y="0" z="3" />
          <transformationref id="y90rot"/>
        </transformation>
        <children>
          <primitiveref id="ovenSide" />
        </children>
      </component>

      <component id="leftOvenTrans">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <translate x="9" y="0" z="0" />
          <transformationref id="y180rot"/>
          <rotate axis="y" angle="90" />
        </transformation>
        <children>
          <primitiveref id="ovenSide" />
        </children>
      </component>

      <component id="chairTopTrans">
        <materials>
            <material id="redMaterial" />
        </materials>
        <texture id="none" />
        <transformation>
          <transformationref id="halfScale" />
          <scale x="1.1" y="0.5" z="1" />
          <translate x="6" y="5" z="13" />
        </transformation>
        <children>
          <componentref id="depthTriangle"/>
        </children>
      </component>

      <component id="chairSecondLeg">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <transformationref id="halfScale" />
          <translate x="8" y="0" z="12.5" />
          <rotate axis="x" angle="-90" />
        </transformation>
        <children>
          <primitiveref id="chairLeg" />
        </children>
      </component>

      <component id="chairFirstLeg">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <transformationref id="halfScale" />
          <translate x="5" y="0" z="12.5" />
          <rotate axis="x" angle="-90" />
        </transformation>
        <children>
          <primitiveref id="chairLeg" />
        </children>
      </component>

      <component id="chairThirdLeg">
        <materials>
            <material id="inherit" />
        </materials>
        <texture id="inherit" />
        <transformation>
          <transformationref id="halfScale" />
          <translate x="6.5" y="0" z="10.5" />
          <rotate axis="x" angle="-90" />
        </transformation>
        <children>
          <primitiveref id="chairLeg" />
        </children>
      </component>
      
    </components>

    
</sxs>